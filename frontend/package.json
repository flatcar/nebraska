{
  "name": "Nebraska",
  "description": "Nebraska is an update manager for Flatcar Container Linux.",
  "private": true,
  "version": "0.1.0",
  "license": {
    "type": "Apache-2.0",
    "url": "http://www.apache.org/licenses/LICENSE-2.0"
  },
  "repository": "https://github.com/flatcar/nebraska",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . && prettier --config package.json --check src/ e2e/",
    "format": "prettier --config package.json --write src/ e2e/",
    "i18n": "npx --no-install i18next ./src/**/ts* ./src/**/**/*.ts* ./src/**/**/**/*.ts* -c ./src/i18n/i18next-parser.config.js",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "cross-env TEST_TZ=true vitest --silent",
    "test:coverage": "cross-env TEST_TZ=true vitest --silent --coverage --environment=jsdom --reporter=default --coverage.dir=coverage/report",
    "test-storybook": "TZ='UTC' test-storybook",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "tsc": "tsc",
    "build-icons": "node tools/build-icons.js ./src/img ./src/icons && npm run format"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.16.11",
    "@mui/material": "^5.17.1",
    "@mui/styles": "^5.16.11",
    "@reduxjs/toolkit": "^2.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^22.14.1",
    "@types/pubsub-js": "^1.8.6",
    "@types/react": "^18.3.17",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^18.3.5",
    "@types/react-window": "^1.8.8",
    "@types/semver": "^7.7.0",
    "@types/yup": "^0.32.0",
    "cross-env": "^7.0.3",
    "dompurify": "^3.2.4",
    "downshift": "^9.0.9",
    "eventemitter3": "^5.0.1",
    "formik": "^2.4.6",
    "formik-mui": "^4.0.0",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "jwt-decode": "^4.0.0",
    "moment-timezone": "^0.5.48",
    "prop-types": "^15.8.1",
    "pubsub-js": "^1.9.5",
    "react": "^18.3.1",
    "react-color": "^2.19.3",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.4.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.1.2",
    "react-window": "^1.8.11",
    "recharts": "^2.15.2",
    "semver": "^7.7.1",
    "underscore": "^1.13.7",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.1",
    "@iconify/icons-mdi": "^1.2.48",
    "@iconify/react": "^5.2.1",
    "@iconify/tools": "^4.1.2",
    "@playwright/test": "^1.51.1",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/addon-links": "^8.5.8",
    "@storybook/manager-api": "^8.5.8",
    "@storybook/node-logger": "^8.5.8",
    "@storybook/preview-api": "^8.5.8",
    "@storybook/react": "^8.5.8",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.5.8",
    "@storybook/test-runner": "^0.21.3",
    "@storybook/theming": "^8.5.8",
    "@testing-library/react": "^16.3.0",
    "@types/glob": "^8.1.0",
    "@types/pg": "^8.11.11",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/underscore": "^1.13.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "^3.1.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "i18next-parser": "^9.3.0",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "pg": "^8.14.1",
    "prettier": "^3.5.3",
    "storybook": "^8.5.8",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.4",
    "vitest": "^3.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint -c package.json --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --config package.json --write"
    ],
    "e2e/**/*.{js,jsx,ts,tsx}": [
      "eslint -c package.json --fix"
    ],
    "e2e/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --config package.json --write"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "semi": true,
    "printWidth": 100,
    "trailingComma": "es5"
  }
}
