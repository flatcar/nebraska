{
  "name": "Nebraska",
  "description": "Nebraska is an update manager for Flatcar Container Linux.",
  "private": true,
  "version": "0.1.0",
  "license": {
    "type": "Apache-2.0",
    "url": "http://www.apache.org/licenses/LICENSE-2.0"
  },
  "repository": "https://github.com/flatcar/nebraska",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . && prettier --config package.json --check src/ e2e/",
    "format": "prettier --config package.json --write src/ e2e/",
    "i18n": "npx --no-install i18next ./src/**/ts* ./src/**/**/*.ts* ./src/**/**/**/*.ts* -c ./src/i18n/i18next-parser.config.js",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "cross-env TEST_TZ=true vitest --silent",
    "test:coverage": "cross-env TEST_TZ=true vitest --silent --coverage --environment=jsdom --reporter=default --coverage.dir=coverage/report",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:oidc": "playwright test --config=playwright.oidc.config.ts",
    "test:oidc:ui": "playwright test --config=playwright.oidc.config.ts --ui",
    "test:oidc:headed": "playwright test --config=playwright.oidc.config.ts --headed",
    "test:oidc:debug": "playwright test --config=playwright.oidc.config.ts --debug",
    "build-storybook:ci": "cross-env NODE_ENV=test storybook build --quiet",
    "serve-storybook:ci": "npm exec -- http-server storybook-static --port 6006 --silent",
    "test-storybook:ci": "npx wait-on tcp:127.0.0.1:6006 && cross-env NODE_ENV=test TZ=UTC test-storybook",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "tsc": "tsc",
    "build-icons": "node tools/build-icons.js ./src/img ./src/icons && npm run format"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.1.0",
    "@mui/styled-engine-sc": "^7.3.6",
    "@mui/styles": "^6.5.0",
    "@mui/system": "^7.1.0",
    "@mui/utils": "^7.1.0",
    "@reduxjs/toolkit": "^2.11.2",
    "@types/node": "^25.0.3",
    "@types/react-color": "^3.0.13",
    "@types/semver": "^7.7.1",
    "cross-env": "^10.1.0",
    "dompurify": "^3.3.1",
    "downshift": "^9.0.13",
    "eventemitter3": "^5.0.1",
    "formik": "^2.4.9",
    "formik-mui": "^4.0.0",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "jwt-decode": "^4.0.0",
    "moment-timezone": "^0.6.0",
    "prop-types": "^15.8.1",
    "react": "19.2.3",
    "react-color": "^2.19.3",
    "react-dom": "19.2.3",
    "react-i18next": "^16.5.1",
    "react-redux": "^9.2.0",
    "react-router": "^7.12.0",
    "react-window": "^2.2.4",
    "recharts": "^3.6.0",
    "semver": "^7.7.3",
    "underscore": "^1.13.7",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.1",
    "@iconify/icons-mdi": "^1.2.48",
    "@iconify/react": "^6.0.2",
    "@iconify/tools": "^5.0.2",
    "@playwright/test": "1.57.0",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@storybook/test-runner": "^0.24.2",
    "@testing-library/react": "^16.3.1",
    "@types/pg": "^8.16.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/underscore": "^1.13.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-istanbul": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^10.1.11",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^17.0.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "i18next-parser": "^9.3.0",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "pg": "^8.14.1",
    "playwright": "1.57.0",
    "prettier": "^3.7.4",
    "storybook": "^10.0.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint -c package.json --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --config package.json --write"
    ],
    "e2e/**/*.{js,jsx,ts,tsx}": [
      "eslint -c package.json --fix"
    ],
    "e2e/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --config package.json --write"
    ],
    "e2e-oidc/**/*.{js,jsx,ts,tsx}": [
      "eslint -c package.json --fix"
    ],
    "e2e-oidc/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --config package.json --write"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "semi": true,
    "printWidth": 100,
    "trailingComma": "es5"
  }
}
